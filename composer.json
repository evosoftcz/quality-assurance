{
    "name":"evosoftcz/quality-assurance",
    "description":"evosoft's Quality Assurance",
    "keywords":[
        "qa",
        "phpcs",
        "standards",
        "coding standard"
    ],
    "type":"library",
    "license":"MPL-2.0",
    "homepage":"https://evosoft.cz",
    "authors":[
        {
            "name":"evosoft sro",
            "homepage":"https://evosoft.cz"
        },
        {
            "name":"Dominik ProchÃ¡zka",
            "email":"domm98cz@gmail.com",
            "homepage":"https://domm.cz",
            "role":"Developer"
        }
    ],
    "require-dev":{
        "php":">=7.1.0",
        "squizlabs/php_codesniffer":"^3.7.1",
        "slevomat/coding-standard":"^8.8.0"
    },
    "minimum-stability":"stable",
    "prefer-stable":true,
    "config":{
        "sort-packages":true
    },
    "scripts":{
        "phpstan-app":"vendor/bin/phpstan analyse -c app/phpstan.neon app --level 8 --memory-limit 1024M --no-progress -vvv",
        "phpstan-libs":"vendor/bin/phpstan analyse -c libs/phpstan.neon libs --level 8 --memory-limit 1024M --no-progress -vvv"
    }
}
